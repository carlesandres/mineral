{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "mineral-app#make": {
      "dependsOn": [
        "mineral-web#build-static"
      ]
    },
    "mineral-web#build-static": {
      "dependsOn": [
        "mineral-web#build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    }
  }
}
